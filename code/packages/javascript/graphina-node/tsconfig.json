{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "allowJs": false,
    "module": "CommonJS",
    "incremental": true,
    "sourceMap": true,
    "noImplicitThis": true,
    "strict": true,
    "declaration": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  }
}
